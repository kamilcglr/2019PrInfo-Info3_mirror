<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane fx:id="searchPane" prefHeight="741.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.tse.ProjetInfo3.mvc.controller.SignInController">
    <children>
        <StackPane fx:id="dialogStackPane" prefHeight="741.0" prefWidth="1200.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <AnchorPane fx:id="anchorPane">
                    <children>
                        <GridPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0"
                                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <JFXTextField fx:id="mail" labelFloat="true" promptText="Entrez votre adresse mail"
                                              GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="1"
                                              GridPane.valignment="CENTER"/>
                                <Label fx:id="questionLabel" alignment="CENTER" contentDisplay="CENTER"
                                       prefHeight="34.0" prefWidth="416.0" text="Inscription" textAlignment="CENTER"
                                       GridPane.columnSpan="5" GridPane.halignment="CENTER"
                                       GridPane.valignment="CENTER"/>
                                <JFXButton fx:id="signinButton" contentDisplay="RIGHT" onAction="#signinButtonpressed"
                                           styleClass="logginSigninButtons" text="Valider" GridPane.columnIndex="3"
                                           GridPane.halignment="RIGHT" GridPane.rowIndex="4" GridPane.valignment="TOP">
                                    <graphic>
                                        <FontIcon fx:id="saveIcon" iconLiteral="fas-save" iconSize="16"
                                                  styleClass="logginSigninIcons"/>
                                    </graphic>
                                </JFXButton>
                                <JFXButton fx:id="loginButton" contentDisplay="RIGHT" onAction="#loginButtonPressed"
                                           styleClass="logginSigninButtons" text="Login" GridPane.columnIndex="4"
                                           GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                                    <graphic>
                                        <FontIcon fx:id="logginIcon" iconLiteral="far-user-circle" iconSize="16"
                                                  styleClass="logginSigninIcons"/>
                                    </graphic>
                                </JFXButton>
                                <JFXTextField fx:id="password" labelFloat="true" promptText="Entrez votre mot de passe"
                                              GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.rowIndex="2"
                                              GridPane.valignment="CENTER"/>
                                <JFXTextField fx:id="twitterName" labelFloat="true"
                                              promptText="Entrez votre identifiant" GridPane.columnIndex="1"
                                              GridPane.columnSpan="3" GridPane.rowIndex="3"
                                              GridPane.valignment="CENTER"/>

                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
            </children>
        </StackPane>
    </children>
    <stylesheets>
        <URL value="@styles/searchTab.css"/>
        <URL value="@styles/main.css"/>
    </stylesheets>
</AnchorPane>
